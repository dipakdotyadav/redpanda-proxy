{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "RedpandaProxy": "Information"
    }
  },
  "RedpandaProxy": {
    "ListenPort": 9092,
    "ListenAddress": "0.0.0.0",
    "LoadBalancing": "RoundRobin",
    "ConnectionTimeout": 30000,
    "HealthCheckInterval": 10000,
    "Brokers": [
      {
        "Host": "redpanda-1.example.com",
        "Port": 9092,
        "Weight": 1,
        "Enabled": true
      },
      {
        "Host": "redpanda-2.example.com",
        "Port": 9092,
        "Weight": 1,
        "Enabled": true
      },
      {
        "Host": "redpanda-3.example.com",
        "Port": 9092,
        "Weight": 2,
        "Enabled": true
      }
    ]
  }
}
